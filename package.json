{
    "name": "@prefecthq/prefect-ui-library",
    "version": "2.4.12",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/PrefectHQ/prefect-ui-library"
    },
    "bugs": {
        "url": "https://github.com/PrefectHQ/prefect-ui-library/issues"
    },
    "homepage": "https://github.com/PrefectHQ/prefect-ui-library#readme",
    "keywords": [
        "prefect 2.0",
        "vue3"
    ],
    "scripts": {
        "build:demo": "vite build --mode=demo",
        "build": "vue-tsc && tsc-alias && vite build",
        "changelog": "auto-changelog --package --commit-limit 0",
        "dev": "vue-tsc -w & tsc-alias -w & vite build -w --force",
        "lint:fix": "eslint src --fix",
        "lint": "eslint src",
        "serve": "vite --mode=demo --host",
        "test": "vitest",
        "validate:types": "vue-tsc --noEmit",
        "version": "npm run changelog && git add CHANGELOG.md"
    },
    "main": "./dist/prefect-ui-library.umd.js",
    "module": "./dist/prefect-ui-library.mjs",
    "exports": {
        ".": {
            "import": "./dist/prefect-ui-library.mjs",
            "require": "./dist/prefect-ui-library.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "types": "./dist/types/src/index.d.ts",
    "devDependencies": {
        "@prefecthq/eslint-config": "1.0.31",
        "@types/d3": "7.4.3",
        "@types/lodash.camelcase": "4.3.9",
        "@types/lodash.debounce": "4.0.9",
        "@types/lodash.merge": "4.6.9",
        "@types/node": "^20.10.4",
        "@types/prismjs": "^1.26.3",
        "@vitejs/plugin-vue": "4.5.2",
        "auto-changelog": "^2.4.0",
        "autoprefixer": "10.4.16",
        "eslint": "8.55.0",
        "lodash.camelcase": "4.3.0",
        "lodash.debounce": "4.0.8",
        "postcss": "8.4.32",
        "tailwindcss": "3.3.6",
        "tsc-alias": "1.8.8",
        "typescript": "5.3.3",
        "vite": "5.0.8",
        "vite-svg-loader": "^5.1.0",
        "vitest": "^1.0.4",
        "vue-tsc": "1.8.25"
    },
    "peerDependencies": {
        "@prefecthq/prefect-design": "^2.2.4",
        "@prefecthq/vue-charts": "^2.0.3",
        "@prefecthq/vue-compositions": "^1.6.6",
        "vee-validate": "^4.7.0",
        "vue": "^3.3.4",
        "vue-router": "^4.0.12"
    },
    "dependencies": {
        "@prefecthq/graphs": "2.1.9",
        "@types/lodash.isequal": "4.5.8",
        "axios": "1.6.2",
        "cronstrue": "^2.47.0",
        "d3": "7.8.5",
        "date-fns": "2.30.0",
        "date-fns-tz": "1.3.7",
        "lodash.isequal": "4.5.0",
        "lodash.merge": "4.6.2",
        "prismjs": "1.29.0"
    }
}
