{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "jsx": "preserve",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/types/",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@prefecthq/prefect-design": [
        "../prefect-design/src/index.ts"
      ],
      "@prefecthq/prefect-design/*": [
        "../prefect-design/src/*"
      ],
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    "types": [
      "vite-svg-loader",
      "vite/client"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.d.ts",
    "**/*.vue",
    "**/*.mts",
    "postcss.config.cjs",
    "vite.config.mts"
  ]
}